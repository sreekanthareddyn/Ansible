ingress_security_rules_for_default_sec_list:
  - source: "{{ quad_zero_route }}"
    protocol: "{{ TCP_protocol }}"
    tcp_options:
      destination_port_range:
        min: {{ HTTP_port }}
        max: {{ HTTP_port }}
  - source: "{{ quad_zero_route }}"
    protocol: "{{ TCP_protocol }}"
    tcp_options:
      destination_port_range:
        min: {{ HTTPS_port }}
        max: {{ HTTPS_port }}
  - source: "{{ quad_zero_route }}"
    protocol: "{{ TCP_protocol }}"
    tcp_options:
      destination_port_range:
        min: {{ Oracle_DB_port }}
        max: {{ Oracle_DB_port }}
